{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": ["vite-plugin-arraybuffer/types"],
    "verbatimModuleSyntax": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": { "~/*": [ "src/*" ] },
    "strictNullChecks": true,
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "jsxImportSource": "react"
  }
}
